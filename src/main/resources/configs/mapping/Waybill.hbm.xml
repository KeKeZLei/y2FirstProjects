<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.accp.pojo">

    <class name="Waybill" table="waybill">
        <id name="id" column="id"/>
        <property name="orderid" column="ORDERID"/>
        <property name="totalprice" column="TOTALPRICE"/>
        <property name="insureprice" column="INSUREPRICE"/>
        <property name="goodsprice" column="GOODSPRICE"/>
        <property name="volume" column="VOLUME"/>
        <property name="weight" column="WEIGHT"/>
        <property name="remark" column="REMARK"/>
        <property name="createtime" column="CREATETIME"/>
        <property name="deliveryspotid" column="DELIVERYSPOTID"/>
    </class>
</hibernate-mapping>