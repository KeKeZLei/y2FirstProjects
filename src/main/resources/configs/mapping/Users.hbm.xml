<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.accp.pojo" auto-import="true">

    <class name="Users" table="users">
        <id name="userid" column="USERID"/>
        <property name="username" column="USERNAME"/>
        <property name="passowrd" column="PASSOWRD"/>
        <property name="powerid" column="POWERID"/>
        <property name="deliveryspotid" column="DELIVERYSPOTID"/>
        <property name="empid" column="EMPID"/>
    </class>
</hibernate-mapping>