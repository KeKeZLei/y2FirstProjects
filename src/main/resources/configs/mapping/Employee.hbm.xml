<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.accp.pojo">

    <class name="Employee" table="employee">
        <id name="empid" column="EMPID"/>
        <property name="empno" column="EMPNO"/>
        <property name="ename" column="ENAME"/>
        <property name="genoer" column="GENOER"/>
        <property name="birthday" column="BIRTHDAY"/>
        <property name="address" column="ADDRESS"/>
        <property name="tel" column="TEL"/>
        <property name="startwordtime" column="STARTWORDTIME"/>
        <property name="salary" column="SALARY"/>
        <property name="email" column="EMAIL"/>
        <property name="cardid" column="CARDID"/>
        <property name="psoitionid" column="PSOITIONID"/>
        <property name="deliveryspotid" column="DELIVERYSPOTID"/>
        <property name="flag" column="FLAG"/>
    </class>
</hibernate-mapping>