<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.accp.pojo.Receipt" table="receipt" schema="logistics">
        <id name="receiptid" column="RECEIPTID"/>
        <property name="carno" column="CARNO"/>
        <property name="deliveryspotid" column="DELIVERYSPOTID"/>
        <property name="empid" column="EMPID"/>
        <property name="createtime" column="CREATETIME"/>
        <property name="remark" column="REMARK"/>
        <property name="flag" column="FLAG"/>
        <property name="toname" column="TONAME"/>
        <property name="ptyoe" column="PTYOE"/>
    </class>
</hibernate-mapping>