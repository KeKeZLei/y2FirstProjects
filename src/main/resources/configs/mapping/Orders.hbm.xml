<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.accp.pojo" auto-import="true">

    <class name="Orders" table="orders">
        <id name="orderid" column="ORDERID"/>
        <property name="orderno" column="ORDERNO"/>
        <property name="sendname" column="SENDNAME"/>
        <property name="sendaddress" column="SENDADDRESS"/>
        <property name="sendtel" column="SENDTEL"/>
        <property name="sendmobtle" column="SENDMOBTLE"/>
        <property name="sendpostcode" column="SENDPOSTCODE"/>
        <property name="receivename" column="RECEIVENAME"/>
        <property name="receiveaddress" column="RECEIVEADDRESS"/>
        <property name="receivetel" column="RECEIVETEL"/>
        <property name="receivemodile" column="RECEIVEMODILE"/>
        <property name="receivepostcode" column="RECEIVEPOSTCODE"/>
        <property name="orderstateid" column="ORDERSTATEID"/>
    </class>
</hibernate-mapping>