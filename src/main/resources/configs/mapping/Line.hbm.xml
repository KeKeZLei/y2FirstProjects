<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.accp.pojo" auto-import="true">

    <class name="Line" table="line">
        <id name="lineid" column="LINEID"/>
        <property name="lineno" column="LINENO"/>
        <property name="linename" column="LINENAME"/>
        <property name="length" column="LENGTH"/>
    </class>
</hibernate-mapping>